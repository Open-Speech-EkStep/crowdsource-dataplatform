<link rel="preload" as="style" onload='this.onload=null,this.rel="stylesheet"' type="text/css" href="../../css/common/homeCard.css">
<div class="col-12 col-md-5 col-lg-3 text-dark mt-4 card card1 mr-lg-3 mr-md-3 justify-content-center align-items-center py-3" id="boloCard">
    <div id="say_container" class="row align-items-center card_container px-0 justify-content-center" style="height:100%">
        <div class="col-12 px-0 text-center">
            <h2 class="display-5">
                <span class="text-primary"><%= __(MODULE) %></span><span><%= __(' India') %></span>
            </h2>
            <div><a href="<%= url %>">
                    <img src="<%= img %>" alt="speaker" id="module_home" class="cursor-pointer listen-logo img-fluid" width="140px" height="140px">
                </a>
            </div>
            <div class="mt-3 font-weight-normal">
                <%= __(P1) %>
            </div>
            <div class="mt-3 font-weight-normal">
                <%= __(P2) %>
            </div>
        </div>
    </div>
</div>
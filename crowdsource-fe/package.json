{
  "name": "crowdsource-fe",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.16.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src public",
    "test": "jest --watch",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:lh-ci": "rimraf .lighthouseci/ && cross-env LIGHTHOUSE_CI_PORT=3000 lhci --config=./.lighthouserc.js autorun",
    "check": "npm run lint && cross-env CI=true npm run test:coverage"
  },
  "dependencies": {
    "bootstrap": "^5.1.0",
    "classnames": "^2.3.1",
    "next": "11.1.0",
    "next-i18next": "^8.6.0",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@lhci/cli": "^0.8.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^27.0.6",
    "jest-axe": "^5.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^0.6.4",
    "next-router-mock": "^0.2.2",
    "prettier": "^2.3.2",
    "puppeteer": "^10.2.0",
    "rimraf": "^3.0.2"
  }
}

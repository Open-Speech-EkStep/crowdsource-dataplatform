@objects
    header      css   header[data-testid=Header]
    section     css   section[data-testid=Hero]
    caraousel   css   div[data-testid=InitiativesCarousel]
    contributionstats   css div[data-testid=ContributionStats]
    badesgroup  css    div[data-testid=BadgesIntro]
    bronzebadge css    div[data-testid="BronzeContribute"]
= Skeleton =
    contributionstats:
        above badesgroup
        below caraousel

    badesgroup:
        above bronzebadge
        inside screen 32px left right

    header:
        above section
        height 72px
        inside screen 0px top left right

    section, caraousel, contributionstats, bronzebadge:
        inside screen 44px left right

    section:
        above caraousel

    header, badesgroup, section, caraousel, contributionstats, bronzebadge:
        centered horizontally inside screen 1px    